{
    "AvailableLanguages": [ "Java", "ObjC", "CSharp", "Python" ],
    "TargetLanguage": "Java",
    "Plugins": [ ],
    "PrimaryNamespaceName": "oneDrive",
    "NamespacePrefix": "OD",
    "InitializeCollections": "false",
    "AllowShortActions": "false",
    "NamespaceOverride": "",
    "TemplatesDirectory": "..\\..\\..\\..\\Templates",
    "DefaultFileCasing": "UpperCamel",
    "TemplateMapping": {
        "Shared": [
            { "Template": "EntityType", "SubProcessor": "EntityType", "Type": "Model", "Name": "<Class>" },
            { "Template": "ComplexType", "SubProcessor": "ComplexType", "Type": "Model", "Name": "<Class>" },
            { "Template": "EnumType", "SubProcessor": "EnumType", "Type": "Model", "Name": "<Class>" },
            { "Template": "EntityClient", "SubProcessor": "EntityContainer", "Type": "Client", "Name": "<Container>Client" },
            { "Template": "EntityRequest", "SubProcessor": "EntityType", "Type": "Request", "Name": "<Class>Request" },
            { "Template": "EntityRequestBuilder", "SubProcessor": "EntityType", "Type": "Request", "Name": "<Class>RequestBuilder" },
            { "Template": "StreamRequest", "SubProcessor": "Property", "Type": "Request", "Name": "<Class><Property>Request", "Include": "content" },
            { "Template": "StreamRequestBuilder", "SubProcessor": "Property", "Type": "Request", "Name": "<Class><Property>RequestBuilder", "Include": "content" },
            { "Template": "MethodRequest", "SubProcessor": "NonCollectionMethod", "Type": "Request", "Name": "<Class><Method>Request" },
            { "Template": "MethodRequestBuilder", "SubProcessor": "NonCollectionMethod", "Type": "Request", "Name": "<Class><Method>RequestBuilder" }
        ],
        "CSharp": [
            { "Template": "IEntityClient", "SubProcessor": "EntityContainer", "Type": "Client", "Name": "<Container>Client" },
            { "Template": "IEntityRequest", "SubProcessor": "EntityType", "Type": "Request", "Name": "I<Class>Request" },
            { "Template": "IEntityRequestBuilder", "SubProcessor": "EntityType", "Type": "Request", "Name": "I<Class>RequestBuilder" },
            { "Template": "EntityCollectionPage", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionPage", "Exclude": "nextExpectedRanges;roles" },
            { "Template": "IEntityCollectionPage", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "I<Property>CollectionPage", "Exclude": "nextExpectedRanges;roles" },
            { "Template": "EntityCollectionRequest", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionRequest", "Exclude": "nextExpectedRanges;roles" },
            { "Template": "IEntityCollectionRequest", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "I<Property>CollectionRequest", "Exclude": "nextExpectedRanges;roles" },
            { "Template": "EntityCollectionRequestBuilder", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionRequestBuilder", "Exclude": "nextExpectedRanges;roles" },
            { "Template": "IEntityCollectionRequestBuilder", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "I<Property>CollectionRequestBuilder", "Exclude": "nextExpectedRanges;roles" },
            { "Template": "EntityCollectionResponse", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionReponse", "Exclude": "nextExpectedRanges;roles" },
            { "Template": "IStreamRequest", "SubProcessor": "Property", "Type": "Request", "Name": "I<Class><Property>Request", "Include": "content" },
            { "Template": "IStreamRequestBuilder", "SubProcessor": "Property", "Type": "Request", "Name": "I<Class><Property>RequestBuilder", "Include": "content" },
            { "Template": "IMethodRequest", "SubProcessor": "NonCollectionMethod", "Type": "Request", "Name": "I<Class><Method>Request" },
            { "Template": "IMethodRequestBuilder", "SubProcessor": "NonCollectionMethod", "Type": "Request", "Name": "I<Class><Method>RequestBuilder" },
            { "Template": "IMethodCollectionRequest", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "I<Class><Method>CollectionRequest" },
            { "Template": "MethodCollectionRequest", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "<Class><Method>CollectionRequest" },
            { "Template": "MethodCollectionRequestBuilder", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "<Class><Method>CollectionRequestBuilder" },
            { "Template": "IMethodCollectionRequestBuilder", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "I<Class><Method>CollectionRequestBuilder" },
            { "Template": "MethodRequestBody", "SubProcessor": "MethodWithBody", "Type": "Model", "Name": "<Class><Method>RequestBody" },
            { "Template": "MethodCollectionResponse", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "<Class><Method>CollectionResponse" },
            { "Template": "MethodCollectionPage", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "<Class><Method>CollectionPage" },
            { "Template": "IMethodCollectionPage", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "I<Class><Method>CollectionPage" },
            { "Template": "SharedCSharp", "SubProcessor": "Other", "Type": "Shared" }
        ],
        "ObjC": [
            { "Template" : "Models", "SubProcessor" : "Other", "Type": "Model", "Name" : "Models"}
        ],
        "Java": {
        },
        "Python": [
            { "Template": "EntityCollection", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>_collection", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "EntityCollectionMethod", "SubProcessor": "Method", "Type": "Request", "Name": "<Class>_<Method>_collection", "Exclude": "nextExpectedRanges;roles", "Matches": "specialCollection"},
            { "Template": "EntityRequest", "SubProcessor": "EntityType", "Type": "Request", "Name": "<Class>_request", "Exclude": "nextExpctedRanges;roles" },
            { "Template": "EntityRequestBuilder", "SubProcessor": "EntityType", "Type": "Request", "Name": "<Class>_request_builder", "Exclude": "nextExpectedRanges;roles" },
            { "Template": "EntityRequest", "SubProcessor": "ComplexType", "Type": "Request", "Name": "<Class>_request", "Matches": "navigable" },
            { "Template": "EntityRequestBuilder", "SubProcessor": "ComplexType", "Type": "Request", "Name": "<Class>_request_builder", "Matches": "navigable" },
            { "Template": "Method", "SubProcessor": "Method", "Type": "Request", "Name": "<Class>_<Method>" },
            { "Template": "SpecialMethod", "SubProcessor": "Method", "Type": "Request", "Name": "<Class>_<Method>", "Matches": "specialMethod" },
            { "Template": "Stream", "SubProcessor": "Property", "Type": "Request", "Name": "<Class>_<Property>_request", "Include": "content" },
            { "Template": "__init__", "SubProcessor": "Other", "Type": "Other", "Name": "__init__" }
        ]
    }

}
