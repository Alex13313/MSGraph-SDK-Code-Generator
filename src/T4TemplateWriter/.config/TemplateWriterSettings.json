    "TemplateMapping": {
        "Shared": [
            { "Template": "EntityType", "SubProcessor": "EntityType", "Type": "Model", "Name": "<Class>" },
            { "Template": "ComplexType", "SubProcessor": "ComplexType", "Type": "Model", "Name": "<Class>", "Ignore": "methodResponse"},
            { "Template": "EnumType", "SubProcessor": "EnumType", "Type": "Model", "Name": "<Class>" },
            { "Template": "EntityClient", "SubProcessor": "EntityContainer", "Type": "Client", "Name": "<Container>Client" },
            { "Template": "EntityRequest", "SubProcessor": "EntityType", "Type": "Request", "Name": "<Class>Request" },
            { "Template": "EntityRequestBuilder", "SubProcessor": "EntityType", "Type": "Request", "Name": "<Class>RequestBuilder" },
            { "Template": "StreamRequest", "SubProcessor": "Property", "Type": "Request", "Name": "<Class><Property>Request", "Include": "content" },
            { "Template": "MethodRequest", "SubProcessor": "Method", "Type": "Request", "Name": "<Class><Method>Request" },
            { "Template": "MethodRequestBuilder", "SubProcessor": "Method", "Type": "Request", "Name": "<Class><Method>RequestBuilder" }
        ],
        "CSharp": [
            { "Template": "EntityType", "SubProcessor": "ComplexType", "Type": "Model", "Name": "<Class>", "Matches":  "navigable" },
            { "Template": "IEntityClient", "SubProcessor": "EntityContainer", "Type": "Client", "Name": "I<Container>Client" },
            { "Template": "IEntityRequest", "SubProcessor": "EntityType", "Type": "Request", "Name": "I<Class>Request" },
            { "Template": "IEntityRequest", "SubProcessor": "ComplexType", "Type": "Request", "Name": "I<Class>Request", "Matches": "navigable" },
            { "Template": "EntityRequest", "SubProcessor": "ComplexType", "Type": "Request", "Name": "<Class>Request", "Matches": "navigable" },
            { "Template": "IEntityRequestBuilder", "SubProcessor": "EntityType", "Type": "Request", "Name": "I<Class>RequestBuilder" },
            { "Template": "IEntityRequestBuilder", "SubProcessor": "ComplexType", "Type": "Request", "Name": "I<Class>RequestBuilder", "Matches": "navigable" },
            { "Template": "EntityRequestBuilder", "SubProcessor": "ComplexType", "Type": "Request", "Name": "<Class>RequestBuilder", "Matches": "navigable" },
            { "Template": "EntityCollectionPage", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionPage", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "IEntityCollectionPage", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "I<Property>CollectionPage", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "EntityCollectionRequest", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionRequest", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "IEntityCollectionRequest", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "I<Property>CollectionRequest", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "EntityCollectionRequestBuilder", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionRequestBuilder", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "IEntityCollectionRequestBuilder", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "I<Property>CollectionRequestBuilder", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "EntityCollectionResponse", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionResponse", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "IStreamRequest", "SubProcessor": "Property", "Type": "Request", "Name": "I<Class><Property>Request", "Include": "content" },
            { "Template": "IStreamRequestBuilder", "SubProcessor": "Property", "Type": "Request", "Name": "I<Class><Property>RequestBuilder", "Include": "content" },
            { "Template": "StreamRequestBuilder", "SubProcessor": "Property", "Type": "Request", "Name": "<Class><Property>RequestBuilder", "Include": "content" },
            { "Template": "IMethodRequest", "SubProcessor": "Method", "Type": "Request", "Name": "I<Class><Method>Request" },
            { "Template": "IMethodRequestBuilder", "SubProcessor": "Method", "Type": "Request", "Name": "I<Class><Method>RequestBuilder" },
            { "Template": "MethodRequestBody", "SubProcessor": "MethodWithBody", "Type": "Model", "Name": "<Class><Method>RequestBody" },
            { "Template": "MethodCollectionResponse", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "<Class><Method>CollectionResponse", "Ignore": "ignorableCollection" },
            { "Template": "MethodCollectionResponse", "SubProcessor": "Method", "Type": "Request", "Name": "<Class><Method>CollectionResponse", "Matches": "specialCollection", "Ignore": "ignorableCollection" },
            { "Template": "MethodCollectionPage", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "<Class><Method>CollectionPage", "Ignore": "ignorableCollection" },
            { "Template": "MethodCollectionPage", "SubProcessor": "Method", "Type": "Request", "Name": "<Class><Method>CollectionPage", "Matches": "specialCollection", "Ignore": "ignorableCollection" },
            { "Template": "IMethodCollectionPage", "SubProcessor": "CollectionMethod", "Type": "Request", "Name": "I<Class><Method>CollectionPage", "Ignore": "ignorableCollection" },
            { "Template": "IMethodCollectionPage", "SubProcessor": "Method", "Type": "Request", "Name": "I<Class><Method>CollectionPage", "Matches": "specialCollection", "Ignore": "ignorableCollection" },
            { "Template": "SharedCSharp", "SubProcessor": "Other", "Type": "Shared" }
        ],
        "ObjC": [
            { "Template": "Models", "SubProcessor": "Other", "Type": "Model", "Name": "Models" },
            { "Template": "ODataEntities", "SubProcessor": "Other", "Type": "Model", "Name": "ODataEntities" },
            { "Template": "EntityCollectionRequest", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionRequest", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "EntityCollectionRequestBuilder", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>CollectionRequestBuilder", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "MethodCollection", "SubProcessor": "Method", "Type": "Request", "Name": "<Class><Method>Collection", "Matches": "specialCollection" }
        ],
        "Java": {
        },
        "Python": [
            { "Template": "EntityCollection", "SubProcessor": "CollectionProperty", "Type": "Request", "Name": "<Property>_collection", "Exclude": "nextExpectedRanges;roles", "Ignore": "ignorableCollection" },
            { "Template": "EntityCollectionMethod", "SubProcessor": "Method", "Type": "Request", "Name": "<Class>_<Method>_collection", "Exclude": "nextExpectedRanges;roles", "Matches": "specialCollection" },
            { "Template": "EntityRequest", "SubProcessor": "EntityType", "Type": "Request", "Name": "<Class>_request", "Exclude": "nextExpctedRanges;roles" },
            { "Template": "EntityRequestBuilder", "SubProcessor": "EntityType", "Type": "Request", "Name": "<Class>_request_builder", "Exclude": "nextExpectedRanges;roles" },
            { "Template": "EntityRequest", "SubProcessor": "ComplexType", "Type": "Request", "Name": "<Class>_request", "Matches": "navigable" },
            { "Template": "EntityRequestBuilder", "SubProcessor": "ComplexType", "Type": "Request", "Name": "<Class>_request_builder", "Matches": "navigable" },
            { "Template": "Method", "SubProcessor": "Method", "Type": "Request", "Name": "<Class>_<Method>" },
            { "Template": "SpecialMethod", "SubProcessor": "Method", "Type": "Request", "Name": "<Class>_<Method>", "Matches": "specialMethod" },
            { "Template": "Stream", "SubProcessor": "Property", "Type": "Request", "Name": "<Class>_<Property>_request", "Include": "content" },
            { "Template": "__init__", "SubProcessor": "Other", "Type": "Other", "Name": "__init__" }
        ]
    }

}
