<#@ template debug="true" hostspecific="true" language="C#" #>
<#@ include file="IEntityRequest.Base.template.tt"#>
<#
OdcmClass entity = host.CurrentType.AsOdcmClass();
var entityName = entity.Name.ToCheckedCase();
var entityRequest = entityName + "Request";

var projection = host.CurrentType.Projection;
var canExpand = projection.SupportsExpand();
var canSelect = projection.BooleanValueOf("Selectable") != false;

#>

namespace <#=this.GetNamespaceName(entity)#>
{
    using System;
    using System.IO;
    using System.Net.Http;
    using System.Threading;
    using System.Threading.Tasks;

    <#=this.GetEntityWithReferenceRequestInterfaceDefinition(entity)#>
    {
        <#=this.GetEntityGetAsyncMethod(entity)#>

<#
    if (canExpand)
    {
        Write("        ");
        Write(this.GetEntityWithReferenceExpandMethod(entity));
        Write("\n\n");
    }

    if (canSelect)
    {
        Write("        ");
        Write(this.GetEntityWithReferenceSelectMethod(entity));
        Write("\n\n");
    }
#>
    }
}
