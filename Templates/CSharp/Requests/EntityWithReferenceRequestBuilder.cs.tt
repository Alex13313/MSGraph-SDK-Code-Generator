#>

namespace <#=this.GetNamespaceName(entity)#>
{
    using System;
    using System.Collections.Generic;

    <#=this.GetEntityWithReferenceRequestBuilderClassDefinition(entity)#>
    {

        <#=this.GetEntityWithReferenceRequestBuilderConstructor(entity)#>

        <#=this.GetEntityWithReferenceRequestMethod(entity)#>

        <#=this.GetEntityWithReferenceRequestMethodWithOptions(entity)#>
        
        <#=this.GetEntityReferenceRequestBuilderProperty(entity)#>
    <#

    if (entity.Kind == OdcmClassKind.MediaEntity || entity.GetProperties(typeName:"Stream").Any())
    {
    #>

        <#=this.GetStreamProperties(entity)#>
    <#
    }
    #>

    }
